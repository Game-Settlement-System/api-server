<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gss.web.mapper.MemberMapper">
	<insert id="joinUp" parameterType="com.gss.web.domain.Member">
		INSERT INTO gssuser (email, userpassword, userphone)
		values(#{email},#{userpassword},#{userphone})
	</insert>
	<select id="checkEmail" parameterType="String" resultType="com.gss.web.domain.Member">
		select * from gssuser where email=#{email}
	</select>
</mapper>